enum Permission {
  ADMIN
  USER
  ESCORT
  AGENT
}

type User {
  id: ID! @unique
  email: String! @unique
  nickname: String! @unique
  password: String!
  name: String
  posts: [Post!]!
  permissions: [Permission]
  profile: SetCard # Id... of Setcard 12345
}

type SetCard {
  id: ID! @unique # 12345
  aliasName: String! @unique
  pictures: [Pictures!]! # array -> ID [124, 2345, 4567, 8901]
  age: Int!
  escort: User! # 12345
}

type Pictures {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  url: String!
  setCard: SetCard!
}

type Post {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  published: Boolean! @default(value: "false")
  title: String!
  content: String
  author: User!
}
