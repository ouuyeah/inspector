enum Permission {
  ADMIN
  DRIVER
  AGENT
}

type User {
  id: ID! @unique
  cc: Int! @unique
  email: String! @unique
  nickname: String! @unique
  password: String!
  name: String
  inspections: [Inspection!]!
  permissions: [Permission]
}

type Inspection {
  id: ID! @unique
  source: String!
  record: String!
  licensePlate: String!
  user: User!
}
