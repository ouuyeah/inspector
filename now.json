{
  "version": 2,
  "name": "inspector",
  "regions": ["bru"],
  "builds": [
    {
      "src": "/frontend/next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/frontend/_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "headers": {
        "cache-control": "immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
